{
    "manifest_version": 2,
    "name": "Fake Profile Detection",
    "description": "This extension lets you detect fake profiles!",
    "version": "1.0",

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "ForR"
    },
    "icons": { "16": "images/icons8-fraud-48.png",
           "48": "images/icons8-fraud-48.png",
          "128": "images/icons8-fraud-96.png" },
    "permissions": [
        "<all_urls>","activeTab","webNavigation","http://localhost/*"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/; object-src 'self'"
}

